<!DOCTYPE struts PUBLIC    
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"    
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="contractPlan" namespace="/contractPlan" extends="cuteframework-default">

		<action name="findContractPlanByPage" class="contractPlanAction" method="findContractPlanByPage">
			<result>/jsp/contractPlan/contractPlanList.jsp</result>
		</action>
		
		<action name="showAdd" class="contractPlanAction" method="showAdd">
			<result>/jsp/contractPlan/contractPlanAdd.jsp</result>
		</action>
		<action name="saveAdd" class="contractPlanAction" method="saveAdd">
			<result>/jsp/success.jsp</result>
		</action>
		
		<action name="showEdit" class="contractPlanAction" method="showEdit">
			<result>/jsp/contractPlan/contractPlanEdit.jsp</result>
		</action>
		<action name="saveEdit" class="contractPlanAction" method="saveEdit">
			<result>/jsp/success.jsp</result>
		</action>

		<action name="showView" class="contractPlanAction" method="showView">
			<result>/jsp/contractPlan/contractPlanView.jsp</result>
		</action>
		
		<action name="delete" class="contractPlanAction" method="delete"/>
				
		
	</package>
</struts>
	
